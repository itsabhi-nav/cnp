#include <stdio.h>
#include <string.h>
#include <ctype.h>

void main() {
    char seq[36] = "qwertyuiopasdfghjklzxcvbnm1234567890";
    char data[100];
    char decoded[100];
    int i, j, len, found;

    printf("\nEnter data: ");
    fgets(data, sizeof(data), stdin);
    len = strlen(data);

    for (i = 0; i < len; i++) {
        found = 0;

        for (j = 0; j < 36; j++) {
            if (seq[j] == tolower(data[i])) {
                if (isupper(data[i])) {
                    decoded[i] = 'A' + j;
                } else if (islower(data[i])) {
                    decoded[i] = 'a' + j;
                } else {
                    decoded[i] = '0' + (j - 26);
                }
                found = 1;
                break;
            }
        }

        if (!found) {
            decoded[i] = data[i];
        }
    }

    decoded[len] = '\0';
    printf("\nDecoded string is: %s", decoded);
}
