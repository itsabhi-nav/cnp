#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char seq[] = "qwertyuiopasdfghjklzxcvvnm1234567890";
    char data[50], encoded[50];
    int len;

    printf("Enter data: ");
    fgets(data, sizeof(data), stdin);
    len = strlen(data) - 1;  // Adjust for the newline character

    for (int i = 0; i < len; i++) {
        if (isupper(data[i]))
            encoded[i] = seq[data[i] - 'A'];
        else if (islower(data[i]))
            encoded[i] = toupper(seq[data[i] - 'a']);
        else if (isdigit(data[i]))
            encoded[i] = seq[data[i] - '0' + 26];
        else
            encoded[i] = data[i];
    }
    encoded[len] = '\0';

    printf("Encoded string is: %s\n", encoded);
    return 0;
}
